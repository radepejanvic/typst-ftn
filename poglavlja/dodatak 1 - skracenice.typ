// Овде пишете скраћенице које сте користили у тексту
 
#import "../funkcije.typ": dodatak

// Због аутоматског бројања сваки додатак мора имати лабелу са префиксом
// dodatak- као у наслову испод.
= Списак коришћених скраћеница <dodatak-1>

#dodatak(
    table(
        table.header[*Скраћеница*][*Опис*],

        [OS], [Operating System (оперативни систем)],
        [YAML], [YAML Ain't Markup Language (језик за описивање конфигурационих датотека)],
        [JSON], [JavaScript Object Notation (формат за размену података)],
        [CLI], [Command-Line Interface (интерфејс командне линије)],
        [CPU], [Central Processing Unit (централна процесорска јединица)],
        [RAM], [Random Access Memory (радна меморија)],
        [VMM], [Virtual Machine Monitor (надзорник виртуелних машина/хипервизор)],
        [SSH], [Secure Shell (сигурни протокол за удаљени приступ)],
        [NAT], [Network Address Translation (претварање мрежних адреса)],
        [USB], [Universal Serial Bus (универзална серијска магистрала)],
        [HCL], [HashiCorp Configuration Language (језик за описивање инфраструктуре као кода)],
        [AWS], [Amazon Web Services (Амазон веб сервиси)],
        [GCP], [Google Cloud Platform (Гугл платформа за рачунарство у облаку)],
        [VCS], [Version Control System (систем за контролу верзија)],
        [MB], [Megabyte (мегабајт)],
        [GUI], [Graphical User Interface (графички кориснички интерфејс)],
        [IP], [Internet Protocol (интернет протокол)],
        [API], [Application Programming Interface (апликациони програмски интерфејс)],
        [VM], [Virtual Machine (виртуелна машина)],
        [GPG], [GNU Privacy Guard (сигурносни алат за шифровање и потписивање података)],
        [TTY], [Teletype/TeleTYpe (псеудо терминал/конзола)],
        [PID], [Process Identifier (идентификатор процеса)],
        [NATS], [NATS Messaging System (систем за размену порука)]
    ),
)

